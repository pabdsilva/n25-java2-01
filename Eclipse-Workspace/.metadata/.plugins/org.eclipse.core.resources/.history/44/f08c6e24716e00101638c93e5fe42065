package br.com.senaisp.bauru.secao03.aula04;

public class Exemplo02 {

	public static void main(String[] args) {
		char letra1 = 'A'; //Usa a tabela Ask para considerar a letra indicada pelo número
		char letra2 = 'B';

		System.out.println("" + letra1 + letra2); // Se não somar "" + a soma das letras retorna o número que re´resenta as letras na tabela Ask
	}

}
